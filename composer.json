{
    "name": "snoeren-development/laravel-admin-users",
    "description": "Define and check for admin users in your application!",
    "keywords": [
        "laravel",
        "admin",
        "middleware"
    ],
    "type": "laravel-package",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.9",
        "orchestra/testbench": "^v9.5.2",
        "phpunit/phpunit": "^11.4.3",
        "spatie/macroable": "^2.0.0"
    },
    "autoload": {
        "psr-4": {
            "SnoerenDevelopment\\AdminUsers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SnoerenDevelopment\\AdminUsers\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Snoeren",
            "email": "info@snoerendevelopment.nl"
        }
    ],
    "minimum-stability": "stable",
    "scripts": {
        "test": "vendor/bin/phpunit ./tests && vendor/bin/phpstan"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SnoerenDevelopment\\AdminUsers\\AdminUsersServiceProvider"
            ]
        }
    }
}
