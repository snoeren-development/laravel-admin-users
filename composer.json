{
    "name": "snoeren-development/laravel-admin-users",
    "description": "Define and check for admin users in your application!",
    "keywords": [
        "laravel",
        "admin",
        "middleware"
    ],
    "type": "laravel-package",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "v8.12.1",
        "phpunit/phpunit": "^10.3.5",
        "spatie/macroable": "^2.0.0"
    },
    "autoload": {
        "psr-4": {
            "SnoerenDevelopment\\AdminUsers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SnoerenDevelopment\\AdminUsers\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Snoeren",
            "email": "info@snoerendevelopment.nl"
        }
    ],
    "minimum-stability": "stable",
    "scripts": {
        "test": "vendor/bin/phpunit ./tests"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SnoerenDevelopment\\AdminUsers\\AdminUsersServiceProvider"
            ]
        }
    }
}
